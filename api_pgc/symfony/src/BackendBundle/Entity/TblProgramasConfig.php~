<?php

namespace BackendBundle\Entity;

/**
 * TblProgramasConfig
 */
class TblProgramasConfig
{
    /**
     * @var integer
     */
    private $idProgramaConfig;

    /**
     * @var boolean
     */
    private $permiteNiveles = true;

    /**
     * @var string
     */
    private $nombreProgramaConfig;

    /**
     * @var integer
     */
    private $idHerencia;


    /**
     * Get idProgramaConfig
     *
     * @return integer
     */
    public function getIdProgramaConfig()
    {
        return $this->idProgramaConfig;
    }

    /**
     * Set permiteNiveles
     *
     * @param boolean $permiteNiveles
     *
     * @return TblProgramasConfig
     */
    public function setPermiteNiveles($permiteNiveles)
    {
        $this->permiteNiveles = $permiteNiveles;

        return $this;
    }

    /**
     * Get permiteNiveles
     *
     * @return boolean
     */
    public function getPermiteNiveles()
    {
        return $this->permiteNiveles;
    }

    /**
     * Set nombreProgramaConfig
     *
     * @param string $nombreProgramaConfig
     *
     * @return TblProgramasConfig
     */
    public function setNombreProgramaConfig($nombreProgramaConfig)
    {
        $this->nombreProgramaConfig = $nombreProgramaConfig;

        return $this;
    }

    /**
     * Get nombreProgramaConfig
     *
     * @return string
     */
    public function getNombreProgramaConfig()
    {
        return $this->nombreProgramaConfig;
    }

    /**
     * Set idHerencia
     *
     * @param integer $idHerencia
     *
     * @return TblProgramasConfig
     */
    public function setIdHerencia($idHerencia)
    {
        $this->idHerencia = $idHerencia;

        return $this;
    }

    /**
     * Get idHerencia
     *
     * @return integer
     */
    public function getIdHerencia()
    {
        return $this->idHerencia;
    }
}
