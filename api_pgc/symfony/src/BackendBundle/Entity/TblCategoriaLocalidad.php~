<?php

namespace BackendBundle\Entity;

/**
 * TblCategoriaLocalidad
 */
class TblCategoriaLocalidad
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nombreCatLocalidad;

    /**
     * @var string
     */
    private $codigo;

    /**
     * @var string
     */
    private $iso3;

    /**
     * @var string
     */
    private $iso;

    /**
     * @var \BackendBundle\Entity\TblCategorias
     */
    private $idCategoria;

    /**
     * @var \BackendBundle\Entity\TblCategoriaLocalidad
     */
    private $idPadreCatagoria;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nombreCatLocalidad
     *
     * @param string $nombreCatLocalidad
     *
     * @return TblCategoriaLocalidad
     */
    public function setNombreCatLocalidad($nombreCatLocalidad)
    {
        $this->nombreCatLocalidad = $nombreCatLocalidad;

        return $this;
    }

    /**
     * Get nombreCatLocalidad
     *
     * @return string
     */
    public function getNombreCatLocalidad()
    {
        return $this->nombreCatLocalidad;
    }

    /**
     * Set codigo
     *
     * @param string $codigo
     *
     * @return TblCategoriaLocalidad
     */
    public function setCodigo($codigo)
    {
        $this->codigo = $codigo;

        return $this;
    }

    /**
     * Get codigo
     *
     * @return string
     */
    public function getCodigo()
    {
        return $this->codigo;
    }

    /**
     * Set iso3
     *
     * @param string $iso3
     *
     * @return TblCategoriaLocalidad
     */
    public function setIso3($iso3)
    {
        $this->iso3 = $iso3;

        return $this;
    }

    /**
     * Get iso3
     *
     * @return string
     */
    public function getIso3()
    {
        return $this->iso3;
    }

    /**
     * Set iso
     *
     * @param string $iso
     *
     * @return TblCategoriaLocalidad
     */
    public function setIso($iso)
    {
        $this->iso = $iso;

        return $this;
    }

    /**
     * Get iso
     *
     * @return string
     */
    public function getIso()
    {
        return $this->iso;
    }

    /**
     * Set idCategoria
     *
     * @param \BackendBundle\Entity\TblCategorias $idCategoria
     *
     * @return TblCategoriaLocalidad
     */
    public function setIdCategoria(\BackendBundle\Entity\TblCategorias $idCategoria = null)
    {
        $this->idCategoria = $idCategoria;

        return $this;
    }

    /**
     * Get idCategoria
     *
     * @return \BackendBundle\Entity\TblCategorias
     */
    public function getIdCategoria()
    {
        return $this->idCategoria;
    }

    /**
     * Set idPadreCatagoria
     *
     * @param \BackendBundle\Entity\TblCategoriaLocalidad $idPadreCatagoria
     *
     * @return TblCategoriaLocalidad
     */
    public function setIdPadreCatagoria(\BackendBundle\Entity\TblCategoriaLocalidad $idPadreCatagoria = null)
    {
        $this->idPadreCatagoria = $idPadreCatagoria;

        return $this;
    }

    /**
     * Get idPadreCatagoria
     *
     * @return \BackendBundle\Entity\TblCategoriaLocalidad
     */
    public function getIdPadreCatagoria()
    {
        return $this->idPadreCatagoria;
    }
}
