<?php

namespace BackendBundle\Entity;

/**
 * TblProyectoLocalidad
 */
class TblProyectoLocalidad
{
    /**
     * @var integer
     */
    private $idProyectoLocalidad;

    /**
     * @var integer
     */
    private $idProyecto;

    /**
     * @var float
     */
    private $porcentajePart;

    /**
     * @var \BackendBundle\Entity\TblLocalidades
     */
    private $idLocalidad;


    /**
     * Get idProyectoLocalidad
     *
     * @return integer
     */
    public function getIdProyectoLocalidad()
    {
        return $this->idProyectoLocalidad;
    }

    /**
     * Set idProyecto
     *
     * @param integer $idProyecto
     *
     * @return TblProyectoLocalidad
     */
    public function setIdProyecto($idProyecto)
    {
        $this->idProyecto = $idProyecto;

        return $this;
    }

    /**
     * Get idProyecto
     *
     * @return integer
     */
    public function getIdProyecto()
    {
        return $this->idProyecto;
    }

    /**
     * Set porcentajePart
     *
     * @param float $porcentajePart
     *
     * @return TblProyectoLocalidad
     */
    public function setPorcentajePart($porcentajePart)
    {
        $this->porcentajePart = $porcentajePart;

        return $this;
    }

    /**
     * Get porcentajePart
     *
     * @return float
     */
    public function getPorcentajePart()
    {
        return $this->porcentajePart;
    }

    /**
     * Set idLocalidad
     *
     * @param \BackendBundle\Entity\TblLocalidades $idLocalidad
     *
     * @return TblProyectoLocalidad
     */
    public function setIdLocalidad(\BackendBundle\Entity\TblLocalidades $idLocalidad = null)
    {
        $this->idLocalidad = $idLocalidad;

        return $this;
    }

    /**
     * Get idLocalidad
     *
     * @return \BackendBundle\Entity\TblLocalidades
     */
    public function getIdLocalidad()
    {
        return $this->idLocalidad;
    }
}
