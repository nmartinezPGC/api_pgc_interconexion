<?php

namespace BackendBundle\Entity;

/**
 * TblProyectoPrograma
 */
class TblProyectoPrograma
{
    /**
     * @var integer
     */
    private $idProyectoPrograma;

    /**
     * @var integer
     */
    private $idProyecto;

    /**
     * @var \BackendBundle\Entity\TblProgramas
     */
    private $idPrograma;

    /**
     * @var \BackendBundle\Entity\TblProgramasConfig
     */
    private $idProgramaConfig;


    /**
     * Get idProyectoPrograma
     *
     * @return integer
     */
    public function getIdProyectoPrograma()
    {
        return $this->idProyectoPrograma;
    }

    /**
     * Set idProyecto
     *
     * @param integer $idProyecto
     *
     * @return TblProyectoPrograma
     */
    public function setIdProyecto($idProyecto)
    {
        $this->idProyecto = $idProyecto;

        return $this;
    }

    /**
     * Get idProyecto
     *
     * @return integer
     */
    public function getIdProyecto()
    {
        return $this->idProyecto;
    }

    /**
     * Set idPrograma
     *
     * @param \BackendBundle\Entity\TblProgramas $idPrograma
     *
     * @return TblProyectoPrograma
     */
    public function setIdPrograma(\BackendBundle\Entity\TblProgramas $idPrograma = null)
    {
        $this->idPrograma = $idPrograma;

        return $this;
    }

    /**
     * Get idPrograma
     *
     * @return \BackendBundle\Entity\TblProgramas
     */
    public function getIdPrograma()
    {
        return $this->idPrograma;
    }

    /**
     * Set idProgramaConfig
     *
     * @param \BackendBundle\Entity\TblProgramasConfig $idProgramaConfig
     *
     * @return TblProyectoPrograma
     */
    public function setIdProgramaConfig(\BackendBundle\Entity\TblProgramasConfig $idProgramaConfig = null)
    {
        $this->idProgramaConfig = $idProgramaConfig;

        return $this;
    }

    /**
     * Get idProgramaConfig
     *
     * @return \BackendBundle\Entity\TblProgramasConfig
     */
    public function getIdProgramaConfig()
    {
        return $this->idProgramaConfig;
    }
}
